@prefix service: <http://example.org/ties4520/tasks/bookingService.owl#> .
@prefix sswap: <http://sswapmeet.sswap.info/sswap/> .
@prefix resource: <http://localhost:8000/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

resource:cottage-booking-service
    a sswap:Resource, service:CottageBookingService ;
    sswap:name "Cottage Booking Service" ;
    sswap:oneLineDescription "Search and book available cottages based on criteria" ;
    sswap:providedBy resource:cottage-booking-service ;
    sswap:operatesOn [
        a sswap:Graph ;
        sswap:hasMapping [
            a sswap:Subject, service:SearchRequest ;
            service:bookerName "Teppo Testaja" ;
            service:nearestCity "Kuopio" ;
            service:distanceToCity 50 ;
            service:distanceToLake 50 ;
            service:bedrooms 2 ;
            service:capacity 2 ;
            service:startDate "2023-03-08"^^xsd:date ;
            service:duration 2 ;
            service:shift 2 ;
            sswap:mapsTo [
                a sswap:Object, service:BookingSuggestion
            ]
        ]
    ] .
